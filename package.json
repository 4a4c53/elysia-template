{
	"name": "elysia-template",
	"version": "0.0.0",
	"module": "src/index.js",
	"packageManager": "bun@1.3.1",
	"license": "MIT",
	"description": "A simple Elysia template with TypeScript setup",
	"author": {
		"name": "Jos√© L Silva",
		"email": "joseluis@4a4c53.com",
		"url": "http://4a4c53.com"
	},
	"config": {
		"port": "4453",
		"outfile": "server"
	},
	"scripts": {
		"build": "bun build --compile --minify-whitespace --minify-syntax --target bun --outfile $npm_package_config_outfile src/index.ts",
		"build:vercel": "tsdown src/index.ts -d dist --dts",
		"build:linux": "bun build --compile --minify-whitespace --minify-syntax --target bun-linux-x64 --outfile $npm_package_config_outfile src/index.ts",
		"build:mac": "bun build --compile --minify-whitespace --minify-syntax --target bun-darwin-x64 --outfile $npm_package_config_outfile src/index.ts",
		"build:mac-arm": "bun build --compile --minify-whitespace --minify-syntax --target bun-darwin-arm64 --outfile $npm_package_config_outfile src/index.ts",
		"build:windows": "bun build --compile --minify-whitespace --minify-syntax --target bun-win32-x64 --outfile $npm_package_config_outfile.exe src/index.ts",
		"build:windows-arm": "bun build --compile --minify-whitespace --minify-syntax --target bun-win32-arm64 --outfile $npm_package_config_outfile.exe src/index.ts",
		"start": "bun run build && ./$npm_package_config_outfile",
		"dev": "PORT=$npm_package_config_port bun run --watch src/index.ts",
		"lint": "biome check",
		"format": "biome format --write"
	},
	"dependencies": {
		"elysia": "1.4.15"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.4",
		"bun-types": "1.3.1",
		"tsdown": "0.16.1",
		"typescript": "5.9.3"
	}
}
